{
  "name": "GitHub Activity Monitor",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "A Microsoft Edge extension to display my activity on the GitHub.",
  "icons": {
    "16": "icons/remember-me-16x16.png",
    "32": "icons/remember-me-32x32.png",
    "48": "icons/remember-me-48x48.png",
    "128": "icons/remember-me-128x128.png"
  },
  "browser_action": {
    "default_popup": "popup/popup.html"
  }
}
